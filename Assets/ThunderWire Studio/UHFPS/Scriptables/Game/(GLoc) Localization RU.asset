    - Key: lost-daughter
      Text: "\u041F\u0440\u043E\u043F\u0430\u0432\u0448\u0430\u044F \u0434\u043E\u0447\u044C"
    - Key: swamp
      Text: "\u0427\u0451\u0440\u0442 \u0431\u044B \u043F\u043E\u0431\u0440\u0430\u043B
        \u044D\u0442\u0438 \u0431\u043E\u043B\u043E\u0442\u0430"
    - Key: swamp.valve
      Text: "\u0413\u0434\u0435-\u0442\u043E \u043D\u0430 \u0431\u043E\u043B\u043E\u0442\u0430\u0445
        \u0437\u0430\u0442\u0435\u0440\u044F\u043B\u0441\u044F \u0432\u0435\u043D\u0442\u0438\u043B\u044C"
    - Key: swamp.fuel
      Text: "\u0427\u0442\u043E\u0431\u044B \u0437\u0430\u043F\u0443\u0441\u0442\u0438\u0442\u044C
        \u0433\u0435\u043D\u0435\u0440\u0430\u0442\u043E\u0440, \u043C\u043D\u0435
        \u043F\u043E\u043D\u0430\u0434\u043E\u0431\u0438\u0442\u044C\u0441\u044F
        \u0442\u043E\u043F\u043B\u0438\u0432\u043E. \u041D\u0430 \u0431\u043E\u043B\u043E\u0442\u0430\u0445
        \u0438\u0441\u043A\u0430\u0442\u044C \u0441\u043C\u044B\u0441\u043B\u0430
        \u043D\u0435\u0442."
    - Key: swamp.water
      Text: "\u0422\u0435\u043F\u0435\u0440\u044C \u0443 \u043C\u0435\u043D\u044F
        \u0435\u0441\u0442\u044C \u0432\u0441\u0451 \u043D\u0435\u043E\u0431\u0445\u043E\u0434\u0438\u043C\u043E\u0435,
        \u043F\u043E\u0440\u0430 \u043E\u0442\u043A\u0430\u0447\u0430\u0442\u044C
        \u0432\u043E\u0434\u0443."
    - Key: swamp.start
      Text: "\u041F\u0440\u0435\u0433\u0440\u0430\u0434\u0430 \u043A \u0431\u043E\u043B\u043E\u0442\u0430\u043C"
    - Key: swamp.sickle
      Text: "\u041F\u0440\u043E\u0445\u043E\u0434 \u043D\u0430 \u0431\u043E\u043B\u043E\u0442\u0430
        \u0437\u0430\u0440\u043E\u0441 \u043A\u0430\u043C\u044B\u0448\u0430\u043C\u0438...
        \u041D\u0443\u0436\u043D\u043E \u043D\u0430\u0439\u0442\u0438 \u0447\u0442\u043E-\u0442\u043E
        \u0447\u0435\u043C \u043C\u043E\u0436\u043D\u043E \u0431\u044B\u043B\u043E
        \u0431\u044B \u043E\u0441\u0432\u043E\u0431\u043E\u0434\u0438\u0442\u044C
        \u043F\u0440\u043E\u0445\u043E\u0434. \u0414\u0443\u043C\u0430\u044E, \u0441\u0435\u0440\u043F
        \u043F\u043E\u0434\u043E\u0439\u0434\u0435\u0442."
    - Key: swamp.enter
      Text: "\u0422\u0435\u043F\u0435\u0440\u044C, \u043A\u043E\u0433\u0434\u0430
        \u0443 \u043C\u0435\u043D\u044F \u0435\u0441\u0442\u044C \u0441\u0435\u0440\u043F,
        \u044F \u0441\u043C\u043E\u0433\u0443 \u043F\u0440\u0435\u043E\u0434\u043E\u043B\u0435\u0442\u044C
        \u043F\u0440\u0435\u0433\u0440\u0430\u0434\u0443 \u0438 \u043F\u043E\u043F\u0430\u0441\u0442\u044C
        \u043D\u0430 \u0442\u0435\u0440\u0440\u0438\u0442\u043E\u0440\u0438\u044E
        \u0431\u043E\u043B\u043E\u0442."
    - Key: village.closed-houses
      Text: "\u0427\u0442\u043E \u0432 \u0434\u043E\u043C\u0430\u0445 \u0442\u0430\u0438\u0442\u0441\u044F"
    - Key: village.lockpicks
      Text: "\u0427\u0442\u043E\u0431\u044B \u0432\u0437\u043B\u043E\u043C\u0430\u0442\u044C
        \u0437\u0430\u043A\u0440\u044B\u0442\u044B\u0435 \u0434\u0432\u0435\u0440\u0438,
        \u043C\u043D\u0435 \u043F\u043E\u043D\u0430\u0434\u043E\u0431\u044F\u0442\u0441\u044F
        \u043E\u0442\u043C\u044B\u0447\u043A\u0438. \u0412\u0441\u043A\u0440\u044B\u0442\u044B\u0435
        \u0434\u043E\u043C\u0430: [count]/3"
    - Key: boatstation
      Text: "\u0421\u0435\u0437\u0430\u043C, \u043E\u0442\u043A\u0440\u043E\u0439\u0441\u044F"
    - Key: boatstation.energy
      Text: "\u041D\u0430 \u043B\u043E\u0434\u043E\u0447\u043D\u043E\u0439 \u0441\u0442\u043E\u0438\u0442
        \u044D\u043B\u0435\u043A\u0442\u0440\u043E\u043D\u043D\u044B\u0439 \u0437\u0430\u043C\u043E\u043A,
        \u043C\u043E\u0436\u043D\u043E \u0437\u0430\u043F\u0438\u0442\u0430\u0442\u044C
        \u0435\u0433\u043E \u0441 \u043F\u043E\u043C\u043E\u0449\u044C\u044E \u044D\u043B\u0435\u043A\u0442\u0440\u043E\u0441\u0442\u0430\u043D\u0446\u0438\u0438
        \u0431\u043B\u0438\u0437 \u043F\u043E\u043B\u044F."
    - Key: boatstation.combination
      Text: "\u0412\u043E\u0440\u043E\u0442\u0430 \u043A \u044D\u043B\u0435\u043A\u0442\u0440\u043E\u0441\u0442\u0430\u043D\u0446\u0438\u0438
        \u0437\u0430\u043A\u0440\u044B\u0442\u044B \u043D\u0430 \u043A\u043E\u0434\u043E\u0432\u044B\u0439
        \u0437\u0430\u043C\u043E\u043A, \u043C\u043E\u0436\u0435\u0442 \u0443 \u043C\u0435\u043D\u044F
        \u043F\u043E\u043B\u0443\u0447\u0438\u0442\u0441\u044F \u043F\u043E\u0434\u044B\u0441\u043A\u0430\u0442\u044C
        \u0432 \u043E\u043A\u0440\u0435\u0441\u0442\u043D\u043E\u0441\u0442\u044F\u0445
        \u043D\u0443\u0436\u043D\u0443\u044E \u043A\u043E\u043C\u0431\u0438\u0430\u043D\u0446\u0438\u044E."
    - Key: boatstation.puzzles
      Text: "\u0427\u0442\u043E\u0431\u044B \u043F\u0440\u043E\u0432\u0435\u0441\u0442\u0438
        \u044D\u043B\u0435\u043A\u0442\u0440\u0438\u0447\u0435\u0441\u0442\u0432\u043E
        \u043A \u043B\u043E\u0434\u043E\u0447\u043D\u043E\u0439 \u0441\u0442\u0430\u043D\u0446\u0438\u0438,
        \u043F\u0440\u0438\u0434\u0435\u0442\u0441\u044F \u043F\u043E\u0440\u0430\u0431\u043E\u0442\u0430\u0442\u044C
        \u044D\u043B\u0435\u043A\u0442\u0440\u0438\u043A\u043E\u043C."
    - Key: boatstation.nums
      Text: "\u0414\u0432\u0435\u0440\u044C \u0437\u0430\u043F\u0435\u0440\u0442\u0430
        \u043D\u0430 \u044D\u043B\u0435\u043A\u0442\u0440\u043E\u043D\u043D\u044B\u0439
        \u0437\u0430\u043C\u043E\u043A, \u043C\u043E\u0436\u0435\u0442 \u043A\u0442\u043E-\u0442\u043E
        \u0438\u0437 \u043C\u0435\u0441\u0442\u043D\u044B\u0445 \u043E\u0441\u0442\u0430\u0432\u0438\u043B
        \u0433\u0434\u0435-\u043D\u0438\u0431\u0443\u0434\u044C \u0437\u0430\u043F\u0438\u0441\u043A\u0443
        \u0441 \u043F\u0440\u0430\u0432\u0438\u043B\u044C\u043D\u043E\u0439 \u043A\u043E\u043C\u0431\u0438\u043D\u0430\u0446\u0438\u0435\u0439."
    - Key: lighthouse
      Text: "\u041C\u0430\u044F\u043A \u0437\u043E\u0432\u0435\u0442"
    - Key: lighthouse.boat
      Text: "\u042F \u043C\u043E\u0433\u0443 \u043D\u0430\u0439\u0442\u0438 \u043B\u043E\u0434\u043A\u0443
        \u043D\u0430 \u0431\u0435\u0440\u0435\u0433\u0443 \u043B\u043E\u0434\u043E\u0447\u043D\u043E\u0439
        \u0441\u0442\u0430\u043D\u0446\u0438\u0438."
    - Key: lighthouse.fuel
      Text: "\u041D\u0443\u0436\u043D\u043E \u043D\u0430\u0439\u0442\u0438 \u043A\u0430\u043D\u0438\u0441\u0442\u0440\u0443
        \u0441 \u0431\u0435\u043D\u0437\u0438\u043D\u043E\u043C."
    - Key: lighthouse.keys
      Text: "\u041A\u0443\u0434\u0430 \u0436\u0435 \u043F\u043E\u0434\u0435\u0432\u0430\u043B\u0438\u0441\u044C
        \u043A\u043B\u044E\u0447\u0438... \u0411\u0435\u0437 \u043D\u0438\u0445 \u043C\u043D\u0435
        \u043D\u0435 \u0437\u0430\u0432\u0435\u0441\u0442\u0438 \u043C\u043E\u0442\u043E\u0440\u043D\u0443\u044E
        \u043B\u043E\u0434\u043A\u0443."
    - Key: lighthouse.final
      Text: "\u0412\u0440\u0435\u043C\u044F \u043F\u0440\u0438\u0448\u043B\u043E,
        \u043D\u0430\u0434\u0435\u044E\u0441\u044C, \u044F \u043D\u0430\u0439\u0434\u0443
        \u0442\u0430\u043C \u0442\u043E, \u0447\u0442\u043E \u0438\u0449\u0443."
    - Key: lost-daughter.start
      Text: "\u041D\u0430\u0439\u0442\u0438 \u043D\u0430\u0441\u0442\u0435\u043D\u044C\u043A\u0443"
    - Key: lost-daughter.end
      Text: "\u041F\u0440\u0438\u043D\u044F\u0442\u044C \u0441\u0432\u043E\u044E
        \u0441\u0443\u0434\u044C\u0431\u0443"
    - Key: idols
      Text: "\u0427\u0442\u043E \u0437\u0430 \u0442\u0443\u043C\u0430\u043D\u0430\u043C\u0438
        \u0441\u043A\u0440\u044B\u0442\u043E"
    - Key: idols.items
      Text: "\u0414\u0443\u043C\u0430\u044E, \u0442\u0443\u043C\u0430\u043D \u0440\u0430\u0441\u0441\u0435\u0438\u0442\u0441\u044F,
        \u0435\u0441\u043B\u0438 \u044F \u043F\u043E\u0434\u043D\u0435\u0441\u0443
        \u0438\u0434\u043E\u043B\u0430\u043C \u043F\u0440\u0430\u0432\u0438\u043B\u044C\u043D\u044B\u0435
        \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u044B"
